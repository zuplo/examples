{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["id", "userId", "title", "completed"],
  "properties": {
    "id": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "completed": {
      "type": "boolean"
    }
  },
  "examples": [
    {
      "userId": 1,
      "id": 1,
      "title": "Take out the trash",
      "completed": false
    }
  ]
}
